        <activity
            android:name="com.android.clockwork.view.activity.RegisterTypeActivity"
            android:label="@string/title_activity_register_type" >
        </activity>
        <activity
            android:name="com.android.clockwork.view.activity.JSRegisterActivity"
            android:label="@string/title_activity_jsregister" >
